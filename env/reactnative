alias bta="./scripts/run-android-local-unit-tests.sh"
alias btf="buck test ReactAndroid/src/test/java/com/facebook/react/views:views --test-selectors 'com.facebook.react.views.textinput.TextInputTest#testPropsUpdate'"
alias adbg="adbgrep 'TESTING::'"
alias ybuildrn="./gradlew :packages:rn-tester:android:app:installJscDebug"
